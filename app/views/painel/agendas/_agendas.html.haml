- @agendas.order("data ASC").order("atendimento_inicio ASC").each do |agenda|
  -if agenda.status.eql?(I18n.t("agendas.helpers.free"))
    =content_tag(:tr, id: "agenda_id_#{agenda.id}" , class: "info") do
      =render 'agenda_tr', { agenda: agenda }
  -elsif agenda.status.eql?(I18n.t("agendas.helpers.scheduled"))
    =content_tag(:tr, id: "agenda_id_#{agenda.id}" , class: "success") do
      =render 'agenda_tr', { agenda: agenda }
  -elsif agenda.status.eql?(I18n.t("agendas.helpers.didnt_come")) or agenda.status.eql?(I18n.t("agendas.helpers.unmarked_by_pacient")) or agenda.status.eql?(I18n.t("agendas.helpers.unmarked_by_doctor"))
    =content_tag(:tr, id: "agenda_id_#{agenda.id}" , class: "attention") do
      =render 'agenda_tr', { agenda: agenda }
  -elsif agenda.status.eql?(I18n.t("agendas.helpers.attended"))
    =content_tag(:tr, id: "agenda_id_#{agenda.id}" , class: "danger") do
      =render 'agenda_tr', { agenda: agenda }