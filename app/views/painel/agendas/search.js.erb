<%-if !@content.nil?%>
  $("#agenda_content_normal").empty();
  $('.table-agenda').find('tbody').attr("id","agenda_content_<%= @content.id %>");
  $("#agenda_content_<%= @content.id %>").empty();
  $("#agenda_content_<%= @content.id %>").append("<%=j render 'agendas' , { agendas: @agendas } %>");
<%-end-%>
