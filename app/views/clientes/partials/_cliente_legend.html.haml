.panel.panel-default
  .panel-heading
    %label="Dados do Cliente - #{@cliente.id}"
  .panel-body
    .row
      .col-md-3
        =label_tag "Nome: "
        =@cliente.nome
      .col-md-2
        =label_tag "CPF: "
        =@cliente.cpf
      .col-md-2
        =label_tag "Rg: "
        =@cliente.rg
