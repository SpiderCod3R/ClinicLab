=build_object_viewer_toolbar(edit_empresa_exame_procedimento_path(current_user.empresa ,@exame_procedimento), @exame_procedimento, empresa_exame_procedimentos_path(current_user.empresa), nil)
%section{id: "show_exame_procedimento"}
  %div{class: "container"}
    %div{class: "row"}
      %div{class: "col-xs-12 col-sm-12 col-md-12 col-lg-12"}
        %div{class: "row-fluid"}
          %h2 Exibindo Exame Procedimento
          %hr{style: "border: #323232 solid 0.5px;"}
        %div{class: "col-xs-8 col-sm-8 col-md-8 col-lg-8"}
          %div{class: "row-fluid"}
            %div{class: "col-xs-3 col-sm-3 col-md-3 col-lg-3"}
              %strong Código:
              = @exame_procedimento.id
            - if @exame_procedimento.tabela.present?
              %div{class: "col-xs-3 col-sm-3 col-md-3 col-lg-3"}
                %strong Tabela:
                = @exame_procedimento.tabela
            - if @exame_procedimento.codigo_procedimento.present?
              %div{class: "col-xs-6 col-sm-6 col-md-6 col-lg-6"}
                %strong Código Procedimento:
                = @exame_procedimento.codigo_procedimento
          %div{class: "row-fluid"}
            %div{class: "col-xs-12 col-sm-12 col-md-12 col-lg-12"}
              %strong Descrição:
              = @exame_procedimento.descricao
