<%- if @changed -%>
  $("#agenda_attended").modal('hide')

  $("#agenda_id_<%= @agenda.id %>").fadeOut(500, function(){
    $(this).empty();
    $(this).prepend("<%= j render 'agenda_tr' , { agenda: @agenda } %>");
    $(this).fadeIn(500).removeClass("success").addClass("danger");
  });

  toastr.success('<%= I18n.t("agendas.messages.service_attended")%>', 'Atenção', {timeOut: 5000});
<%- end -%>