<%-if !@content.nil?%>
  $("#<%= I18n.t('agendas.helpers.default_content')%>").empty();
  $('.table-agenda').find('tbody').attr("id","<%= @content %>");
  $("#<%= @content %>").empty();
  $("#<%= @content %>").append("<%=j render 'agendas' , { agendas: @agendas } %>");
<%- else -%>
  $('.table-agenda').find('tbody').attr("id", "<%= I18n.t('agendas.helpers.default_content')%>")
  $("#<%= I18n.t('agendas.helpers.default_content')%>").empty();
  $("#<%= I18n.t('agendas.helpers.default_content')%>").append("<%=j render 'agendas' , { agendas: @agendas } %>");
<%-end-%>


<% if @agendas.empty? %>
  $(".lendodados").text('NÃ£o existem mais dados.');
  $(".lendodados").fadeIn(1000);
<%- else -%>
  $(".lendodados").empty();
  $(".lendodados").append("<%=j render 'application/componentes/rolagem' %>").fadeIn(1000);
  $(".lendodados").fadeOut(1000);
<% end %>